@model IEnumerable<AgeType>

@{
    ViewData["Title"] = "Index";
}

<div class="alert alert-info " role="alert">
    <p class="d-flex justify-content-center" style="font-size:larger">Type of Ages</p>
</div>

<p>
    <a asp-action="Create" class="btn btn-primary"><i class="bi bi-plus-circle"></i> Create New</a>
</p>
@{if (ViewBag.success !=null)
{
    <div class="alert alert-warning" role="alert">
            <i class="bi bi-exclamation-diamond"></i> You dont have products!
        </div>
        
    }else{
<table class="table table-striped">
    <thead >
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AgeTypeId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type_Title)
            </th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AgeTypeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type_Title)
            </td>
         
            <td>
                            <a class="btn btn-primary" asp-action="Edit" asp-route-Id="@item.AgeTypeId">Edit</a>
               
            </td>
        </tr>
}
    </tbody>
</table>
}}